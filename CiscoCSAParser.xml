    <Model>Generic</Model>
    <Version>ANY</Version>
  </deviceType>

  <appType>
    <Vendor>Cisco</Vendor>
    <Model>CSA Management Center</Model>
    <Version>ANY</Version>
    <Name>Cisco CSA Management Center</Name>
  </appType>

  <eventFormatRecognizer><![CDATA[SNMPv2-SMI::enterprises.8590.3.1]]></eventFormatRecognizer>

  <testEvents>
