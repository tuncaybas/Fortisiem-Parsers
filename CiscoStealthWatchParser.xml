  <parsingInstructions>
    <switch>
      <case>
        <collectFieldsByRegex src="$_rawmsg">
          <regex><![CDATA[<:gPatSyslogPRI>?\s*<:gPatMon>\s+<:gPatDay>\s+<:gPatTime>\s+<:gPatStr>\s+<:patStealthwatch>\[<:gPatInt>\]:\s+<:patStrEndSep>\|<:patStealthwatch>\|PRIORITY\s+<:patStrEndSep>\|<_body:gPatMesgBody>]]></regex>
        </collectFieldsByRegex>
        <collectFieldsByKeyValuePair kvsep="=" sep="|" src="$_body">
          <attrKeyMap attr="_deviceTime" key="time"/>
          <attrKeyMap attr="targetHostName" key="target_hostname"/>
          <attrKeyMap attr="_eventSeverity" key="alarm_severity_id"/>
          <attrKeyMap attr="_eventTypeId" key="alarm_type_id"/>
          <attrKeyMap attr="eventDesc" key="alarm_type_description"/>
          <attrKeyMap attr="ipPort" key="port"/>
          <attrKeyMap attr="destIpAddr" key="target_ip"/>
          <attrKeyMap attr="destMACAddr" key="target_mac_address"/>
          <attrKeyMap attr="hostName" key="source_hostname"/>
          <attrKeyMap attr="srcIpAddr" key="source_ip"/>
          <attrKeyMap attr="srcMACAddr" key="source_mac_address"/>
          <attrKeyMap attr="srcUser" key="source_username"/>
          <attrKeyMap attr="reptDevIpAddr" key="device_ip"/>
          <attrKeyMap attr="reptDevName" key="device_name"/>
          <attrKeyMap attr="details" key="details"/>
          <attrKeyMap attr="_startTime" key="start_active_time"/>
          <attrKeyMap attr="_endTime" key="end_active_time"/>
        </collectFieldsByKeyValuePair>
      </case>
      <case>
        <collectFieldsByRegex src="$_rawmsg">
          <regex><![CDATA[<:gPatSyslogPRI>?\s*<:gPatMon>\s+<:gPatDay>\s+<:gPatTime>\s+<:gPatStr>\s+<:patStealthwatch>\[<:gPatInt>\]:\s+<:patStrEndSep>\|<:patStealthwatch>\|<:patStrEndSep>\|<:patStrEndSep>\|\d+\|<_body:gPatMesgBody>]]></regex>
        </collectFieldsByRegex>
        <collectAndSetAttrByKeyValuePair sep=" " src="$_body">
          <attrKeyMap attr="_eventSeverity" key="severity_id="/>
          <attrKeyMap attr="_eventTypeId" key="alarm_type="/>
          <attrKeyMap attr="_startTime" key="start="/>
          <attrKeyMap attr="_endTime" key="end="/>
          <attrKeyMap attr="destIpAddr" key="dst="/>
          <attrKeyMap attr="destIpPort" key="dpt="/>
          <attrKeyMap attr="srcIpAddr" key="src="/>
          <attrKeyMap attr="description" key="alarm_desc="/>
          <attrKeyMap attr="details" key="details="/>
          <attrKeyMap attr="alertCategory" key="category="/>
          <attrKeyMap attr="alarm" key="Alarm_ID="/>
          <attrKeyMap attr="ipProto" key="proto="/>
          <attrKeyMap attr="domain" key="Domain="/>
          <attrKeyMap attr="signatureName" key="signature="/>
          <attrKeyMap attr="vendorSeverity" key="vendor_severity="/>
        </collectAndSetAttrByKeyValuePair>
      </case>
    </switch>

    <setEventAttribute attr="eventType">Cisco-StealthWatch-Generic</setEventAttribute>

    <when test="exist _deviceTime">
