      <attrKeyMap attr="_objName" key="SNMPv2-SMI::enterprises.674.10893.1.20.200.200.4 = STRING: "/>
      <attrKeyMap attr="_extraData" key="SNMPv2-SMI::enterprises.674.10892.1.5000.10.6.0 = STRING: "/>
