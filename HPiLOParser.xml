  <!-- HP iLO SNMP Trap format  -->
  <eventFormatRecognizer><![CDATA[<:gPatYear>-<:gPatMon>-<:gPatDay>\s+<:gPatTime>\s+<:gPatHostName>.*?TRAP,\s+SNMP\s+<:gPatWord>,\s+(?:community\s<:gPatStr>)\s+SNMPv<:gPatInt>-SMI::enterprises\.232\b]]></eventFormatRecognizer>

  <parsingInstructions>
    <switch>
      <case>
        <collectFieldsByRegex src="$_rawmsg">
          <regex><![CDATA[<_year:gPatYear>-<_mon:gPatMon>-<_day:gPatDay>\s+<_time:gPatTime>\s+(?:<reptDevIpAddr:gPatIpAddr>|<reptDevName:gPatHostName>).*?TRAP,\s+SNMP\s+<version:gPatWord>,\s+community\s+<comm:gPatStr>\s+SNMPv<:gPatInt>-<oid:gPatStr>\s+<_body:gPatMesgBody>]]></regex>
        </collectFieldsByRegex>
      </case>
      <default/>
    </switch>
    <setEventAttribute attr="eventType">HP-Generic</setEventAttribute>

    <choose>
      <when test="$oid = 'SMI::enterprises.232'">
        <!-- parse specific trap id -->
        <switch>
          <case>
            <collectFieldsByRegex src="$_body">
              <regex><![CDATA[Enterprise Specific Trap \(<_id:gPatInt>\)\s+<_bodyDetail:gPatMesgBody>]]></regex>
            </collectFieldsByRegex>
          </case>
          <default/>
        </switch>
      </when>
      <when test="$oid = 'SMI::enterprises.232.9.4.10'">
        <!-- cold start trap -->
        <switch>
          <case>
            <collectFieldsByRegex src="$_body">
              <regex><![CDATA[Cold Start Trap\s+\(<_id:gPatInt>]]></regex>
            </collectFieldsByRegex>
          </case>
          <default/>
        </switch>
      </when>
    </choose>
    <setEventAttribute attr="deviceTime">toDateTime($_mon, $_day, $_year, $_time)</setEventAttribute>
    <when test="exist _bodyDetail">
      <collectAndSetAttrByKeyValuePair sep="\t\\|\s+SNMP" src="$_bodyDetail">
        <attrKeyMap attr="hostName" key="SNMPv2-MIB::sysName.0 = STRING: "/>
        <attrKeyMap attr="hostName" key="SNMPv2-MIB::sysName = STRING: "/>
      </collectAndSetAttrByKeyValuePair>
    </when>

