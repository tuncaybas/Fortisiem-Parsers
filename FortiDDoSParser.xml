<eventParser name="FortiDDoSParser">
  <deviceType>
    <Vendor>Fortinet</Vendor>
    <Model>FortiDDoS</Model>
    <Version>ANY</Version>
  </deviceType>

  <testEvents>
    <testEvent><![CDATA[Jan 10 16:01:50 172.30.84.114 devid=FI400B3913000032 date=2015-01-23 time=17:42:00 type=attack SPP=1 evecode=1 evesubcode=8 dir=0 protocol=1 sIP=0.0.0.0 dIP=0.0.0.0 dropCount=312]]></testEvent>
    <testEvent><![CDATA[devid=FI800B3913000055 date=2017-01-27 time=18:24:00 tz=PST type=attack spp=0 evecode=2 evesubcode=61 description="Excessive Concurrent Connections Per Source flood" dir=1 sip=1.1.1.1 dip=1.1.1.2 subnet_name=default dropcount=40249 facility=Local0 level=Notice]]></testEvent>
  </testEvents>

  <eventFormatRecognizer><![CDATA[\bdevid="?(?:FI|FIVM)-?\d\w*"?\s+date=\d{4}-\d{2}-\d{2}\s+time=<:gPatTime>\s]]></eventFormatRecognizer>

  <parsingInstructions>
    <switch>
      <case>
        <collectFieldsByRegex src="$_rawmsg">
          <regex><![CDATA[<:gPatMon>\s+<:gPatDay>\s+<:gPatTime>\s+(?:<reptDevIpAddr:gPatIpAddr>|<reptDevName:gPatHostName>)\s+<_body:gPatMesgBody>]]></regex>
        </collectFieldsByRegex>
      </case>
      <default>
        <setEventAttribute attr="_body">$_rawmsg</setEventAttribute>
      </default>
    </switch>
    <collectFieldsByKeyValuePair sep=" " kvsep="=" src="$_body">
      <attrKeyMap attr="_date" key="date"/>
      <attrKeyMap attr="_time" key="time"/>
      <attrKeyMap attr="type" key="type"/>
      <attrKeyMap attr="subtype" key="subtype"/>
      <attrKeyMap attr="sppNum" key="SPP"/>
      <attrKeyMap attr="sppNum" key="spp"/>
      <attrKeyMap attr="_eveCode" key="evecode"/>
      <attrKeyMap attr="_eveSubCode" key="evesubcode"/>
      <attrKeyMap attr="direction" key="dir"/>
      <attrKeyMap attr="ipProto" key="protocol"/>
      <attrKeyMap attr="srcIpAddr" key="sIP"/>
      <attrKeyMap attr="srcIpAddr" key="sip"/>
      <attrKeyMap attr="srcIpPort" key="sPort"/>
      <attrKeyMap attr="destIpAddr" key="dIP"/>
      <attrKeyMap attr="destIpAddr" key="dip"/>
      <attrKeyMap attr="destIpPort" key="dPort"/>
      <attrKeyMap attr="icmpType" key="icmpType"/>
      <attrKeyMap attr="icmpCode" key="icmpCode"/>
      <attrKeyMap attr="totPktDrop" key="dropCount"/>
      <attrKeyMap attr="totPktDrop" key="dropcount"/>
      <attrKeyMap attr="logID" key="log_id"/>
      <attrKeyMap attr="_severity" key="pri"/>
      <!-- <attrKeyMap attr="" key="msg_id"/>-->
      <attrKeyMap attr="_user" key="user"/>
      <attrKeyMap attr="infoURL" key="ui"/>
      <attrKeyMap attr="opName" key="action"/>
      <attrKeyMap attr="status" key="status"/>
      <attrKeyMap attr="errReason" key="reason"/>
      <attrKeyMap attr="msg" key="msg"/>
      <attrKeyMap attr="details" key="detail"/>
      <attrKeyMap attr="eventDesc" key="description"/>
    </collectFieldsByKeyValuePair>

    <when test="exist infoURL">
      <setEventAttribute attr="destName">extractHostFromURL($infoURL)</setEventAttribute>
    </when>

    <setEventAttribute attr="eventType">FortiDDoS-Generic</setEventAttribute>
    <collectFieldsByRegex src="$_date">
      <regex><![CDATA[<_year:gPatYear>-<_mon:gPatMonNum>-<_day:gPatDay>]]></regex>
    </collectFieldsByRegex>
    <setEventAttribute attr="deviceTime">toDateTime($_mon, $_day, $_year, $_time)</setEventAttribute>
    <choose>
      <when test="not_exist _user"/>
      <when test="$_user = 'N/A'"/>
      <otherwise>
        <setEventAttribute attr="user">$_user</setEventAttribute>
      </otherwise>
    </choose>
    <choose>
      <when test="not_exist _severity"/>
      <when test="$_severity = 'emergency'">
        <setEventAttribute attr="eventSeverity">9</setEventAttribute>
      </when>
      <when test="$_severity = 'notice'">
        <setEventAttribute attr="eventSeverity">1</setEventAttribute>
      </when>
      <when test="$_severity = 'information'">
        <setEventAttribute attr="eventSeverity">1</setEventAttribute>
      </when>
      <when test="$_severity = 'critical'">
        <setEventAttribute attr="eventSeverity">9</setEventAttribute>
      </when>
      <when test="$_severity = 'alert'">
        <setEventAttribute attr="eventSeverity">7</setEventAttribute>
      </when>
      <when test="$_severity = 'error'">
        <setEventAttribute attr="eventSeverity">5</setEventAttribute>
      </when>
      <when test="$_severity = 'warning'">
        <setEventAttribute attr="eventSeverity">3</setEventAttribute>
      </when>
    </choose>
    <when test="exist _eveCode">
      <when test="exist _eveSubCode">
        <setEventAttribute attr="_eveTypeCode">combineMsgId($_eveCode,"_",$_eveSubCode)</setEventAttribute>
      </when>
    </when>
    <when test="exist _eveTypeCode">
      <choose>
        <when test="$_eveTypeCode = '1_0'">
          <setEventAttribute attr="eventType">FortiDDoS-Protocol-Flood</setEventAttribute>
        </when>
        <when test="$_eveTypeCode = '1_1'">
          <setEventAttribute attr="eventType">FortiDDoS-Fragment-Flood</setEventAttribute>
        </when>
        <when test="$_eveTypeCode = '1_2'">
          <setEventAttribute attr="eventType">FortiDDoS-TOS-Flood</setEventAttribute>
        </when>
        <when test="$_eveTypeCode = '1_3'">
          <setEventAttribute attr="eventType">FortiDDoS-IPV4-Option-Flood</setEventAttribute>
        </when>
        <when test="$_eveTypeCode = '1_4'">
          <setEventAttribute attr="eventType">FortiDDoS-IP-Header-Anomaly</setEventAttribute>
        </when>
        <when test="$_eveTypeCode = '1_5'">
          <setEventAttribute attr="eventType">FortiDDoS-Source-Flood-Start</setEventAttribute>
        </when>
        <when test="$_eveTypeCode = '1_6'">
          <setEventAttribute attr="eventType">FortiDDoS-ST-Hash-Attack</setEventAttribute>
