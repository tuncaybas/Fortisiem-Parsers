      <when test="$_id  = '31011'">
        <setEventAttribute attr="_eventType">"Dvmlog-Debug"</setEventAttribute>
      </when>
      <when test="$_id  = '32002'">
        <setEventAttribute attr="_eventType">"Connection"</setEventAttribute>
      </when>
      <when test="$_id  = '32003'">
        <setEventAttribute attr="_eventType">"Login-Notif"</setEventAttribute>
      </when>
      <when test="$_id  = '32004'">
        <setEventAttribute attr="_eventType">"Login-Error"</setEventAttribute>
      </when>
      <when test="$_id  = '34002'">
        <setEventAttribute attr="_eventType">"Error-Mode"</setEventAttribute>
      </when>
      <when test="$_id  = '34003'">
        <setEventAttribute attr="_eventType">"Enable-Fips-Mode"</setEventAttribute>
      </when>
      <when test="$_id  = '34004'">
        <setEventAttribute attr="_eventType">"Self-Test"</setEventAttribute>
      </when>
      <when test="$_id  = '34005'">
        <setEventAttribute attr="_eventType">"Encryption"</setEventAttribute>
      </when>
      <when test="$_id  = '34006'">
        <setEventAttribute attr="_eventType">"Decryption"</setEventAttribute>
      </when>
      <when test="$_id  = '34007'">
        <setEventAttribute attr="_eventType">"Prng"</setEventAttribute>
      </when>
      <when test="$_id  = '35002'">
        <setEventAttribute attr="_eventType">"Log-View-Notif"</setEventAttribute>
      </when>
      <when test="$_id  = '35003'">
        <setEventAttribute attr="_eventType">"Logdaemon-Updown-Notif"</setEventAttribute>
      </when>
      <when test="$_id  = '36002'">
        <setEventAttribute attr="_eventType">"Reboot"</setEventAttribute>
      </when>
      <when test="$_id  = '36003'">
        <setEventAttribute attr="_eventType">"Shutdown"</setEventAttribute>
      </when>
    </choose>

    <setEventAttribute attr="eventType">combineMsgId("FMGR-","Generic")</setEventAttribute>

    <when test="exist _subtype">
      <switch>
        <case>
          <when test="exist _eventType">
            <setEventAttribute attr="eventType">combineMsgId("FMGR-",$_subtype,"-",$_eventType)</setEventAttribute>
          </when>
        </case>
        <default>
          <setEventAttribute attr="eventType">combineMsgId("FMGR-",$_subtype,"-","Generic")</setEventAttribute>
        </default>
      </switch>
    </when>

    <when test="not_exist usrMsg">
      <when test="exist _msg">
        <setEventAttribute attr="usrMsg">$_msg</setEventAttribute>
      </when>
    </when>

    <when test="not_exist srcIpAddr">
      <when test="exist _userfrom">
        <switch>
          <case>
            <collectFieldsByRegex src="$_userfrom">
              <regex><![CDATA[^<authenMethod:gPatWord>\(<srcIpAddr:gPatIpAddr>\)$]]></regex>
            </collectFieldsByRegex>
          </case>
          <default/>
        </switch>
      </when>
    </when>

    <choose>
      <when test="not_exist _severity"/>
      <when test="$_severity = 'information'">
        <setEventAttribute attr="eventSeverity">1</setEventAttribute>
      </when>
      <when test="$_severity = 'notice'">
        <setEventAttribute attr="eventSeverity">3</setEventAttribute>
      </when>
      <when test="$_severity = 'warning'">
        <setEventAttribute attr="eventSeverity">4</setEventAttribute>
      </when>
      <when test="$_severity = 'error'">
        <setEventAttribute attr="eventSeverity">5</setEventAttribute>
      </when>
      <when test="$_severity = 'critical'">
        <setEventAttribute attr="eventSeverity">7</setEventAttribute>
      </when>
      <when test="$_severity = 'alert'">
        <setEventAttribute attr="eventSeverity">8</setEventAttribute>
      </when>
      <when test="$_severity = 'emergency'">
        <setEventAttribute attr="eventSeverity">9</setEventAttribute>
      </when>
    </choose>
  </parsingInstructions>
</eventParser>
